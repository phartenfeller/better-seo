{
	"name": "better-seo",
	"displayName": "better-seo",
	"description": "Avoid common SEO traps while you write your Blogpost",
	"license": "MIT",
	"author": {
		"email": "jan@work-with-jan.com",
		"name": "Jan Schulte",
		"url": "https://beyondfeaturetalk.com"
	},
	"publisher": "janschulte",
	"repository": {
		"type": "git",
		"url": "https://github.com/schultyy/better-seo"
	},
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:better-seo.analyze"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "better-seo.analyze",
				"title": "Better SEO: Analyze"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "seo-explorer",
					"title": "SEO Explorer",
					"icon": "resources/betterseo.svg"
				}
			]
		},
		"views": {
			"seo-explorer": [
				{
					"id": "frontmatter",
					"name": "Frontmatter"
				},
				{
					"id": "text",
					"name": "Text"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"webpack-dev": "webpack --mode development --watch",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.17",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"webpack": "^5.24.2",
		"webpack-cli": "^4.5.0"
	},
	"dependencies": {
		"@textlint/markdown-to-ast": "^6.3.2",
		"gray-matter": "^4.0.2"
	}
}
